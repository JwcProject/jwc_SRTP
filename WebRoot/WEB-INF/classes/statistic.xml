<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="statistic" extends="struts-default,json-default" namespace="/Statistic">
	
		<action name="ListSchoolProjectScore" class="StatisticAction" method="schoolScoreStatistic">
			<result name="success">/pages/StatisticsManage/iconpie2.jsp</result>
			<result name="noData">../noData_err.jsp</result>
			<result name="error">../db_err.jsp</result>
		</action>
		
		<action name="SchoolResultDistribut" class="StatisticAction" method="schoolResultDistribut">
			<result name="success">/pages/StatisticsManage/school_result_statistic_list.jsp</result>
			<result name="error">../db_err.jsp</result>
		</action>
		
		<action name="CollegeResultDistribut" class="StatisticAction" method="collegeResultDistribut">
			<result name="success">/pages/StatisticsManage/barPages/cylinderlightbar.jsp</result>
			<result name="noData">../noData_err.jsp</result>
			<result name="error">../db_err.jsp</result>
		</action>
		
		<action name="JiciResultDistribut" class="StatisticAction" method="collegeResultDistributByCollege">
			<result name="success">/pages/StatisticsManage/linePages/symbolline.jsp</result>
			<result name="noData">../noData_err.jsp</result>
			<result name="error">../db_err.jsp</result>
		</action>
		
		<action name="IsCollegeNameHadValue" class="StatisticAction" method="isCollegeNameHadValue">
			<result name="success" type="json">
				<param name="contentType">text/html</param>
			    <param name="includeProperties">result</param>
			</result>
			<result name="error" type="json">
				<param name="contentType">text/html</param>
			    <param name="includeProperties">result</param>
			</result>
			<result name="noValue" type="json">
				<param name="contentType">text/html</param>
			    <param name="includeProperties">result</param>
			</result>
		</action>
	</package>
</struts>