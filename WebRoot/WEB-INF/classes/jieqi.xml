<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="jieqi" extends="struts-default,json-default" namespace="/">
		<action name="ListAllJieqi" class="jieqiAction" method="findAllJieQis">
			<result name="success">/pages/systemManage/jieqiManage/jieqi_list.jsp</result>
			<result name="error">db_err.jsp</result>
		</action>

		<action name="PreAddJieqi" class="jieqiAction" method="preAddJieqi">
			<result name="success">/pages/systemManage/jieqiManage/jieqi_add.jsp</result>
			<result name="error">db_err.jsp</result>
		</action>
		
		<action name="AddJieqi" class="jieqiAction" method="addJieqi">
			<result name="success" type="json">
			</result>
			<result name="error"> db_err.jsp</result>
		</action>
		
		<action name="PreUpdateJieqi" class="jieqiAction" method="preUpdateJieqi">
			<result name="success">/pages/systemManage/jieqiManage/jieqi_edit.jsp</result>
			<result name="error">db_err.jsp</result>
		</action>
		
		<action name="UpdateJieqi" class="jieqiAction" method="updateJieqi">
			<result name="success" type="json">
				<param name="contentType">text/html</param>
				<param name="includeProperties ">result</param>
			</result>
			<result name="error" type="json">
				<param name="contentType">text/html</param>
				<param name="includeProperties ">result</param>
			</result>
		</action>
		
		<action name="DeleteJieqi" class="jieqiAction" method="deleteJieqi">
			<result name="success" type="redirect">ListAllJieqi</result>
			<result name="error">db_err.jsp</result>
		</action>
		
	</package>
</struts>